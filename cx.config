{
	"version": 1.0,
	"team": "/CxServer",
	"project": "fef-CxWorkshop",

	"sast": {
		"preset": "High and Medium",
	    "incremental": "false",
	    "forceScan" : "true",
	    "fileExcludes": "cx.config",
		"folderExcludes": ".git,pids,logs,results,build,node_modules,diet,p4,.sass-cache,.DS_Store,.idea,.classpath,.project,.factorypath,.repository,.versionbackup,*test,*tests,dist,maps,.tmp,.grunt,coverage,results,doc,docs"
	},
	
	"additionalProperties" : {
		"cxFlow": {
	      		"filters" : {
	        		"severity": ["High","Medium"],
				"status": ["To Verify","Recurrent"]
	      		}
	    	}
	}
}
